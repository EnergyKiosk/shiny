observeEvent(input$rayshade, {
car <- cars |> filter(model == input$car) #Get point of selected location
selected_solardach <- solardach |>
filter(solardach$GWR_EGID == solardach_index()) |>
st_centroid()
##TODO Berechung stimmt noch nicht
